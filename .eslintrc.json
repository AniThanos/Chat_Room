{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console":"off",
        "linebreak-style":["error","windows"],
        "max-len":"off",
        "indent":"off",
        "comma-dangle":["error","never"]
    }    
}